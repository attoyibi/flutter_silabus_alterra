void main() {
  // Menggunakan data dari data.dart
  for (var product in stockData) {
    print('ID: ${product['id']}');
    print('Nama: ${product['nama']}');
    print('Title: ${product['title']}');
    print('Deskripsi: ${product['deskripsi']}');
    print('Tanggal Masuk: ${product['tanggal_masuk']}');
    print('-----------------------------------');
  }
}
